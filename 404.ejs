<%- await include("partials/header", { bot, user, title: "Not found", is_logged, dashboardDetails, dashboardConfig, hasClientSecret }) %>
  <body style="background-attachment: fixed; background-size: cover; background-position: center; background-image: url('https://www.gifcen.com/wp-content/uploads/2022/04/wallpaper-gif-4.gif');">
<div class="container-sm center" style="margin-top: 25px;">
    <div class="d-grid gap-2 col-6 mx-auto" style="text-align: center;">
        <h1>Uh oh...</h1>
        <h2>The page you requested for was not found.</h2>
    </div>
</div>
<% try { %>
  <% if(dashboardDetails.customData.error.footer.text !== undefined){ %>
    <div style="position: fixed; left: 0; bottom: 0; width: 100%; background-color: <%= dashboardDetails.customData.error.footer.color ? dashboardDetails.customData.error.footer.color : `#622d86` %>; opacity: 0.75; color: white; text-align: center; padding: 3px;">
      <h3><%= dashboardDetails.customData.error.footer.text %></h3>
    </div>
  <% } %>
<% } catch (err) { %>
  <% try { %>
    <div style="position: fixed; left: 0; bottom: 0; width: 100%; background-color: <%= dashboardDetails.customData.all.footer.color ? dashboardDetails.customData.all.footer.color : `#622d86` %>; opacity: 0.75; color: white; text-align: center; padding: 3px;">
      <h3><%= dashboardDetails.customData.all.footer.text %></h3>
    </div>
  <% } catch (err) { %>
  <% }%>
<% }%>
<%- await include("partials/footer") %>